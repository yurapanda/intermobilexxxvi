<%= simple_form_for(@car) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :plate_number %>
    <%= f.association :car_model %>
    <%= f.input :contact_number %>
    <%= f.input :model_year %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
